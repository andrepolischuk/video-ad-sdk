{
  "tags": {
    "allowUnknownTags": false
  },
  "source": {
    "include": ["src", "README.md"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs)"
  },
  "plugins": ["plugins/markdown"],
  "opts": {
    "template": "node_modules/braintree-jsdoc-template",
    "encoding": "utf8",
    "destination": "pages/docs/",
    "private": true,
    "recurse": true,
    "verbose": true
  },
  "templates": {
    "referenceTitle": "video-ad-sdk",
    "disableSort": false,
    "collapse": true,
    "resources": {
      "repo": "https://github.com/rambler-digital-solutions/video-ad-sdk"
    }
  }
}
